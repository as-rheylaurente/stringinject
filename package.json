{
  "name": "stringinject",
  "version": "1.0.0",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "prepare": "npm run build:cjs && npm run build:esm",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc --outDir dist/cjs --module commonjs --project tsconfig.app.json",
    "build:esm": "tsc --outDir dist/esm --module ESNext --project tsconfig.app.json",
    "test": "mocha"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "chai": "^4.3.0",
    "mocha": "^10.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0"
  }
}
